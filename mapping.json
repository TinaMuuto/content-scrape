            "Body": ".inspiration-tile__content"
        }
    },
    {
        "name": "B32 & B72: Content Spotter for PLP",
        "selector": ".inspiration-tile-v2, .category-tile, .usp-spot",
        "components": {
            "Body": "*"
        }
    },
    {
        "name": "B33: Mega link list",
        "selector": ".mega-links",
        "components": {
            "Link List": ".mega-links__list",
            "Image URL": ".mega-links__image-container img[src]"
