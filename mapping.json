[
    {
        "name": "B01: Page Header",
        "selector": ".page-header",
        "components": {
            "Headline": ".page-header__headline",
            "Body": ".page-header__body",
            "Navigation": ".page-header__nav"
        }
    },
    {
        "name": "B01: Page Header / Center aligned",
        "selector": ".page-header--center-aligned",
        "components": {
            "Headline": ".page-header__headline",
            "Body": ".page-header__body",
            "Navigation": ".page-header__nav"
        }
    },
    {
        "name": "B01: Page Header / Flexible",
        "selector": ".page-header--flexible",
        "components": {
            "Headline": ".page-header__headline",
            "Body": ".page-header__body"
        }
    },
    {
        "name": "B01: Article Header",
        "selector": ".article-headline",
        "components": {
            "Headline": "h1"
        }
    },
    {
        "name": "B02: Article Sub-header",
        "selector": ".article-subheadline",
        "components": {
            "Headline": "h2"
        }
    },
    {
        "name": "B03: Article Manchet",
        "selector": ".article-manchet",
        "components": {
            "Body": "div"
        }
    },
    {
        "name": "B04: Article Paragraph",
        "selector": ".article-paragraph",
        "components": {
            "Body": "p"
        }
    },
    {
        "name": "B05: Button (link)",
        "selector": "a.button, a.button--primary, a.button--secondary",
        "components": {
            "CTA Text": null,
            "CTA Link": "[href]"
        }
    },
    {
        "name": "B06: Image with text",
        "selector": ".article-image",
        "components": {
            "Image URL": "img[src]",
            "Caption": "figcaption, .byline",
            "Rich Text": ".rich-text"
        }
    },
    {
        "name": "B07: Images in two columns",
        "selector": ".article-images-2col",
        "components": {
            "Image URL": ".article-images-2col__figure img[src]",
            "Body": ".article-images-2col__text"
        }
    },
    {
        "name": "B08: Interview",
        "selector": ".article-interview",
        "components": {
            "Interview Line": ".article-interview__line"
        }
    },
    {
        "name": "B09: Quote",
        "selector": ".article-quote",
        "components": {
            "Quote": "blockquote",
            "Source": "cite"
        }
    },
    {
        "name": "B10: Rich Text",
        "selector": ".article-richtext",
        "components": {
            "Rich Text": "*"
        }
    },
    {
        "name": "B11: Video (Vimeo)",
        "selector": ".video-player--vimeo",
        "components": {
            "Video URL": "iframe[src]"
        }
    },
    {
        "name": "B12: customer pricelist",
        "selector": ".product-teaser--flip",
        "components": {
            "Image URL": ".product-teaser__image img[src]",
            "Aside Content": ".product-teaser__aside"
        }
    },
    {
        "name": "B13: Market Links",
        "selector": ".links-list--download a",
        "components": {
            "Download Text": null,
            "Download Link": "[href]"
        }
    },
    {
        "name": "B14: Image Text Slider Block",
        "selector": ".slider--with-cta",
        "components": {
            "Image URL": ".product-teaser__image img[src]",
            "Body": ".product-teaser__aside",
            "CTA Text": ".product-teaser__cta",
            "CTA Link": ".product-teaser__cta a[href]"
        }
    },
    {
        "name": "B15: Link Banner",
        "selector": ".category-tile",
        "components": {
            "Headline": ".category-tile__headline",
            "Body": ".category-tile__body",
            "Link List": ".category-tile__list"
        }
    },
    {
        "name": "B16: Links List",
        "selector": ".links-list a",
        "components": {
            "Link Text": null,
            "Link": "[href]"
        }
    },
    {
        "name": "B17 & B22: Related Content",
        "selector": ".recommendation",
        "components": {
            "Headline": "h2, h3",
            "Content": ".inspiration-tile__inner, .product-tile__inner"
        }
    },
    {
        "name": "B23: Three column block",
        "selector": ".grid-spotter",
        "components": {
            "Headline": ".grid-spotter__headline",
            "Image URL": ".grid-spotter__image img[src]",
            "Body": ".grid-spotter__text"
        }
    },
    {
        "name": "B24: related variants",
        "selector": ".recommendation, .product-tile.bordered-grid",
        "components": {
            "Content": ".product-tile__inner"
        }
    },
    {
        "name": "B26: Images in two columns with buttons",
        "selector": ".article-images-2col",
        "components": {
            "Headline": ".article-images-2col__headline",
            "Body": ".article-images-2col__text",
            "Image URL": ".article-images-2col__figure img[src]",
            "CTA Text": "a.button",
            "CTA Link": "a.button[href]"
        }
    },
    {
        "name": "B27: Image slider",
        "selector": ".slider--inspiration",
        "components": {
            "Image URL": ".swiper-slide img[src]"
        }
    },
    {
        "name": "B27_V2: Image slider block",
        "selector": ".slider-v2",
        "components": {
            "Headline": ".slider-v2__header",
            "Image URL": ".slider-v2__image img[src]",
            "Image Caption": ".slider-v2__image-caption"
        }
    },
    {
        "name": "B28: Section Header",
        "selector": ".section-header",
        "components": {
            "Headline": ".section-header__headline",
            "Body": ".section-header__body",
            "CTA Text": "a.button",
            "CTA Link": "a.button[href]"
        }
    },
    {
        "name": "B29: Explore topic",
        "selector": ".explore-topic",
        "components": {
            "Image URL": ".explore-topic__image img[src]",
            "Headline": "h4",
            "Body": "p",
            "CTA Text": "a",
            "CTA Link": "a[href]"
        }
    },
    {
        "name": "B30: Three column navigation block",
        "selector": "a.grid-spotter--button-link",
        "components": {
            "CTA Text": ".grid-spotter__inner",
            "CTA Link": "[href]"
        }
    },
    {
